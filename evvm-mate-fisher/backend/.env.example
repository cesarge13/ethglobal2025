# Relayer Private Key (sin prefijo 0x)
# Esta es la clave privada del wallet que pagará el gas en Polygon Mainnet
RELAYER_PRIVATE_KEY=your_private_key_here_without_0x_prefix

# Polygon Mainnet RPC URL
# Puedes usar el RPC público o un provider como Infura/Alchemy
POLYGON_RPC_URL=https://polygon-rpc.com
# O con Infura:
# POLYGON_RPC_URL=https://polygon-mainnet.infura.io/v3/YOUR_INFURA_KEY
# O con Alchemy:
# POLYGON_RPC_URL=https://polygon-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_KEY

# EVVM Executor Address (opcional, tiene un default)
# Verifica la dirección correcta del executor EVVM en Polygon Mainnet
EVVM_EXECUTOR_ADDRESS=0x9902984d86059234c3B6e11D5eAEC55f9627dD0f

# Puerto del backend (opcional, default: 3001)
PORT=3001
